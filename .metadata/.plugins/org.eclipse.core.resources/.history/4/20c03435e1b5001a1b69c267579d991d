
public class Car {


	
	private String doors ;
	private String engine ;
	private String driver ;
	private int speed;





	

	public int getSpeed() {
		return speed;
	}

	public String getDoors() {
		return doors;
	}

	

	public String getEngine() {
		return engine;
	}

	

	public String getDriver() {
		return driver;
	}



	public String run() {
		
		
		
		if (doors.equals("closed") && engine.equals("on") && driver.equals("seated") && speed > 0) {
			return "running" ;
		
			}else 
			return "not running" ;
	}

}
