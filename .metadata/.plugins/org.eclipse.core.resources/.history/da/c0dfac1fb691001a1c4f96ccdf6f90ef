package zoo;

public class Lions extends Animals implements Mammals, Asia{

	public Lions(String name, int age) {
		super(name, age); 
		
	}
	public void origin() {
		System.out.println("This animal is from Asia");
	}
	@Override
	public String toString() {
		return "Lions [name=" + name + ", age=" + age + "]";
	}
	
	
	
}
